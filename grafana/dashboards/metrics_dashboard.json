{
  "uid": "metrics-overview",
  "title": "App Metrics Overview",
  "timezone": "browser",
  "schemaVersion": 16,
  "version": 1,
  "panels": [
    {
      "type": "graph",
      "title": "Go routines",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "go_goroutines",
          "legendFormat": "goroutines",
          "interval": ""
        }
      ],
      "xaxis": {
        "mode": "time"
      },
      "yaxes": [
        {
          "format": "short",
          "label": null,
          "logBase": 1,
          "max": null,
          "min": null,
          "show": true
        },
        {
          "show": true
        }
      ]
    },
    {
      "type": "stat",
      "title": "HTTP 2xx/4xx/5xx",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[1m])) by (status)",
          "legendFormat": "{{status}}"
        }
      ]
    },
    {
      "type": "heatmap",
      "title": "Request Duration",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[1m])) by (le, path))",
          "legendFormat": "{{path}}"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Requests Per Second",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[1m]))",
          "legendFormat": "RPS"
        }
      ]
    },
  ]
}
